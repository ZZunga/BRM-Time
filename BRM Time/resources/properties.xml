<resources>

    <properties>
        <property id="appVersion" type="string">1.0.8</property>
        <property id="dispDF" type="number">0</property>
        <property id="targetSpeed" type="number">0</property>
        <property id="userSPEED" type="number">15</property>
        <property id="useDist2Dest" type="boolean">false</property>
        <property id="dist2Dest" type="number">200</property>
        <property id="topRight" type="number">0</property>
        
    </properties>

    <settings>
		<setting propertyKey="@Properties.appVersion" title="@Strings.appVersion">
			<settingConfig type="alphaNumeric" readonly="true" />
		</setting>

        <setting propertyKey="@Properties.targetSpeed" title="@Strings.targetSpeed_title">
            <settingConfig type="list">
                <listEntry value="0">@Strings.spdBRM200</listEntry>
                <listEntry value="1">@Strings.spdBRMGR</listEntry>
                <listEntry value="2">@Strings.spdBRMSR</listEntry>
                <listEntry value="3">@Strings.spdGFONDO</listEntry>
                <listEntry value="9">@Strings.spdUSER</listEntry>
            </settingConfig>
        </setting>

        <setting propertyKey="@Properties.userSPEED" title="@Strings.userSPEED_title">
            <settingConfig type="numeric" min="1" max="100" />
        </setting>
		
		<setting propertyKey="@Properties.dispDF" title="@Strings.dispDF_title">
            <settingConfig type="list">
                <listEntry value="0">@Strings.dispTime</listEntry>
                <listEntry value="1">@Strings.dispRemain</listEntry>
                <listEntry value="2">@Strings.dispElapsed</listEntry>
                <listEntry value="3">@Strings.dispERSwap</listEntry>
                <listEntry value="4">@Strings.dispCESwap</listEntry>
                <listEntry value="5">@Strings.dispCRSwap</listEntry>
            </settingConfig>
		</setting>

        <setting propertyKey="@Properties.useDist2Dest" title="@Strings.useDist2Dest_title">
            <settingConfig type="boolean" />
        </setting>
		
        <setting propertyKey="@Properties.dist2Dest" title="@Strings.dist2Dest_title">
            <settingConfig type="numeric" min="1" max="5000" />
        </setting>

        <setting propertyKey="@Properties.topRight" title="@Strings.topRight_title">
            <settingConfig type="list">
                <listEntry value="0">@Strings.battery_title</listEntry>
                <listEntry value="1">@Strings.targetspeed_title</listEntry>
            </settingConfig>
        </setting>
		
	</settings>>

</resources>
